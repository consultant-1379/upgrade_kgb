CI_ADMIN1_HOSTNAME=atrcxb1135
CI_ADMIN2_HOSTNAME=atrcxb1136
CI_ADMIN1_ROOTPW=shroot
CI_ADMIN2_ROOTPW=shroot
CI_ADMIN1_ILO_NAME=10.42.39.142
CI_ADMIN2_ILO_NAME=10.42.39.143
CI_ADMIN1_ILO_ROOTPW=shroot12
CI_ADMIN2_ILO_ROOTPW=shroot12
CI_RELEASE=O13_2
CI_SHIPMENT=13.2.eu
CI_BUILD_TYPE=CU
CI_TYPE=II
CI_OSS_PROD_NUM=
